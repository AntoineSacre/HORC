    <!-- http://www.semanticweb.org/antoine/ontologies/2025/6/EUBindingAct#MDR_Subject_Matter_and_Scope -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/antoine/ontologies/2025/6/EUBindingAct#MDR_Subject_Matter_and_Scope">
        <rdf:type rdf:resource="http://www.semanticweb.org/antoine/ontologies/2025/6/EUBindingAct#Subject_Matter_and_Scope"/>
        <EUBindingAct:part_of_enacting_term rdf:resource="http://www.semanticweb.org/antoine/ontologies/2025/6/EUBindingAct#MDR_Enacting_Term"/>
