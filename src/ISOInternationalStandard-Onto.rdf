    <!-- http://www.semanticweb.org/antoine/ontologies/2025/5/ISO#27001_Applicability -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/antoine/ontologies/2025/5/ISO#27001_Applicability">
